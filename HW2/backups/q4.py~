def q4(n):
    A = 0
    B = 0
    C = 0
    D = 0
    E = 0
    for i in range(0, n):
        A = A + 1
        for j in range(0, i + 1):
            B = B + 1
            if j == i:
                C = C + 1
            for k in range(0, j):
                D = D + 1
                if j >= n - 3:
                    E = E + 1
    return {'A': A, 'B': B, 'C': C, 'D': D, 'E': E}
